body
	background grey
	color white
	font-size 14px
	postion absolute
	margin  0px
	text-align center

	header 
		background black
		display inline-block
		hight auto

		h1
			display inline-block
			font-family "Pinyon Script", Arial
			font-size 1.4em
			postion relative
			width 50%

		#imagen
			float left
			width 20%

		#image 
			float right
			width 20%

		nav
			background #ccc
			border-bottom 4px solid transparent
			border-left 10px solid black
			border-top 4px solid transparent
			border-bottom 4px solid transparent
			border-right 10px solid black
			border-top 4px solid transparent
			color white
			display inline-block
			font-size 1.4em
			text-align center
			vertical-align top
			width 90%

		nav ul
			list-style none
			margin 0px
			padding 0 0.8em

		nav ul li 
			display inline-block
			margin 0 1.2em 0 0
			vertical-align top

		nav ul li a 
			color black
			font-family "Dancing Script", Arial
			font-weight normal
			display block
			text-decoretion none
			padding 0 0

		nav ul li a:hover
			border-bottom 1px solid #0058A5
			margin 0px
			padding 0px

		nav #logo
			display inline-block
			border-radius 50%
			display inline 
			float left
			left 0 
			margin 0 0 0 0 
			postion relative
			top 1em
			text-decoretion none
			width 40px