body
	background grey
	color white
	font-size 14px
	postion absolute
	margin  0px
	text-align center

	header 
		background black
		display inline-block
		hight auto

		h1
			display inline-block
			font-family "Pinyon Script", Arial
			font-size 1.4em
			postion relative
			width 50%

		#imagen
			border-radius 20px
			display inline-block
			height 300px
			width 100%

		#image 
			float right
			width 20%

		nav
			background #ccc
			border-bottom 4px solid transparent
			border-left 10px solid black
			border-top 4px solid transparent
			border-bottom 4px solid transparent
			border-right 10px solid black
			border-top 4px solid transparent
			color white
			display inline-block
			font-size 1.4em
			text-align center
			vertical-align top
			width 90%

		nav ul
			list-style none
			margin 0px
			padding 0 0.8em

		nav ul li 
			display inline-block
			margin 0 1.2em 0 0
			vertical-align top

		nav ul li a 
			color black
			font-family "Dancing Script", Arial
			font-weight normal
			display block
			text-decoretion none
			padding 0 0

		nav ul li a:hover
			border-bottom 1px solid #0058A5
			margin 0px
			padding 0px

		nav #logo
			display inline-block
			border-radius 50%
			display inline 
			float left
			left 0 
			margin 0 0 0 0 
			postion relative
			top 1em
			text-decoretion none
			width 40px

	#contenedor
		background white
		display inline-block
		postion relative
		width 100%
		z-index 1em

		h2
			color black
			font-family "Pinyon Script", Arial
			font-size 1.0em
			float left
			margin 0.3em
			padding 0.3em
			text-align left
			width 90%

		#contenido
			display inline-block
			float left
			margin 0em 0.3em
			top 0px
			width 70%

			h2
				font-family "Pinyon Script", Arial
				font-size 1.3em

			#texto 
				border-top 1px solid black
				color black
				display inline-block
				text-align left
				width 100%

				b 
					font-family "Dancing Script", Arial
					font-size 1.3em
				

		#spam
			display inline-block
			float right
			postion relative
			text-align center
			width 20%

			.lateral
				display inline-block
				font-size 0.8em
				float left
				margin 4em 0.3em
				padding 0.3em 0.3em
				top 2em
				text-align left
				width 60%

				a 
					color black
					display block
					text-decoration none

				a:hover
					color:blue
					font-size 1.2em

			#calendario
				display inline-block
				float left
				margin 1em 1em
				margin-left 0px
				width 100%

				#calen 
					display inline-block
					postion absolute
					font-size 1.2em
					float left
					width 80%

			#pdf
				color black
				display inline-block
				float left
				margin 1em 0em
				postion relative
				text-align left
				width 65%

				h3
					color black
					font-size 1em
					float left
					font-family "Pinyon Script", Arial
					padding 0.4em
					width 80%

				a 
					color black 
					font-size 1.0em
					margin 1em 0em

		#pie
			background black
			display inline-block
			postion relative
			width 100%

			#redes
				float right
				display inline-block
				width 40%

				.social
					vertical-align top
					width 10%

			#avisos
				margin 0.3em 0.3em
				width 80%

				a 
					color white
					font-family "Pinyon Script", Arial
					font-size 1.0em
					padding 0.6em

				a:hover
					font-size 1.3em

			#presentacion
				display inline-block
				float left
				height auto
				vertical-align top
				width 60%

				#medalla
					border-radius 50%
					float left
					margin 0em 0.6em
					postion relative
					width 12% 

			#enlaces
				border-bottom 1px solid white
				border-top 1px solid white
				display inline-block
				float left
				font-size 1.0em
				postion relative
				text-align center
				width 90%

				#izquierda, #centro, #derecha
					vertical-align top
					width 80%

					.enlace 
						display inline-block
						font-size 0.7em
						padding 2em
						text-align center
						vertical-align top
						width 10%

						h3
							font-family "Pinyon Script", Arial
							font-size 1.5em

						a 
							color white
							display block
							font-family 1.0em
							text-decoration none

			#calle
				display inline-block
				height auto
				vertical-align top
				width 90%

				#contacto
					display inline-block 
					top 0px
					width 80%

					a 
						color white
						font-size 0.7em
						text-decoration none

					a:hover
						color #aad
						font-size 0.8em

@import"responsive.styl"
